package Day7;
import java.util.Scanner;
public class Toggle {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		String s=sc.next();
		for(int i=0;i<s.length();i++) {
				char ch=s.charAt(i);
				if((int)ch<97) {
					System.out.print((char)(ch+32));
				}else {
					System.out.print((char)(ch-32));
				}
		}
	}

}
