package Day7;
import java.util.Scanner;
public class method {
public int add(int a,int b) {
	int sum=a+b;
	return sum;
}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int n1=sc.nextInt();
		
		method me=new method();
		int s=me.add(n,n1);
		System.out.println(s);
	}

}
