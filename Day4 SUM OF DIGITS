package Day4;
import java.util.Scanner;
public class sumofdigits {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		while(a>9) {
			int s=0;
			while(a>0) {
				int r=a%10;
				s=s+r;
				a=a/10;
			}a=s;
		}System.out.println(a);
	}
}
