package Day8;

class Dad{
	void property() {
		System.out.println("House");
	}
}

class Son extends  Dad{
	void prop() {
		System.out.println("Cycle");
	}
}
public class inherit {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Son s=new Son();
		s.property();
	}
}
